          <Group Orientation="Horizontal">
            <Label Text="Name" Width="50px" />
            <InputField Id="txtName" Width="150px" ValueChanged="NameChanged" />
          </Group>
          <Group Orientation="Horizontal">
            <Label Text="Gender" Width="150px" />
            <RadioButton IsChecked="{Gender}" Text="Female" />
            <RadioButton IsChecked="{!Gender}" Text="Male" />
          </Group>
          <Button Text="Next" Click="Hair" Width="250px" />
          <Button Text="Back" Click="Back" IsDisabled="{!=CanBack}" Width="250px"/>
        </Group>
        <Group Orientation="Vertical" Spacing="15px">
          <DataGrid Id="DataGrid" Width="250px" Items="{=Hairs}" ItemSelected="HairChanged" IsScrollable="True" ListHeight="200">
            <RowHeader>
              <ColumnHeader Width="*" Text="Hair" />
            </RowHeader>

            <Row IsTemplate="true">
              <Column Text="Hair {=#Item}" />
            </Row>
          </DataGrid>
          <ComboBox Width="250px" Items="{HMaterials}" ComboBoxList.ItemSelected="HMChanged" IsScrollable ="true" IsDropUp="true">
            <ComboBoxItem IsTemplate="true" Text="{#Item}" />
          </ComboBox>
          <Button Text="Next" Click="Advanced" Width="150px" />
          <Button Text="Previous" Click="General" IsDisabled="{!=CanBack}" Width="150px"/>
        </Group>
        <Group Orientation="Vertical" Spacing="15px" Alignment="Center">
